{
  "name": "pdf-ocr-ts",
  "version": "1.0.8",
  "description": "Javascript-only library to perform OCR on scanned PDFs to turn them into searchable PDFs",
  "main": "./build/PdfOcr.js",
  "dependencies": {
    "@types/jest": "^29.5.12",
    "jimp": "^0.22.12",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.0.379",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.5",
    "tesseract.js": "^5.0.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.70",
    "@swc/jest": "^0.2.27",
    "jest": "^29.6.1",
    "typescript": "^4.9.5"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "build": "rimraf ./build && tsc",
    "start": "npx ts-node ./src/ocrMyPdf.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianheide/pdf-ocr-ts.git"
  }
}
